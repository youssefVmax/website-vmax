  "scripts": {
    "build": "next build",
    "dev": "next dev -p 3001",
    "lint": "next lint",
    "start": "next start -p 3001",
    "generate-logos": "node scripts/generate-logos.js",
    "dev:fresh": "powershell -ExecutionPolicy Bypass -File scripts/dev-fresh.ps1",
    "dev:fresh:unix": "chmod +x scripts/dev-fresh.sh && ./scripts/dev-fresh.sh",
    "build:fresh": "powershell -ExecutionPolicy Bypass -File scripts/build-fresh.ps1",
    "build:fresh:unix": "chmod +x scripts/build-fresh.sh && ./scripts/build-fresh.sh",
    "clean": "rimraf .next && rimraf node_modules/.cache && npm cache clean --force",
    "fresh": "npm run clean && npm run dev",
    "build:production": "NODE_ENV=production next build",
    "start:production": "NODE_ENV=production DEPLOY_TARGET=vmaxcom next start -p 3001",
    "deploy:prepare": "npm run clean && npm run build:production",
    "deploy:server": "NODE_ENV=production DEPLOY_TARGET=vmaxcom npm run start"
  },
